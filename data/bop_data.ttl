:non-resident a otv:Attribute,
        owl:DatatypeProperty ;
    rdfs:label "non-resident" ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( :Entity_entitlement_to_own_assets_ability_to_take_economic_decisions_ability_to_enter_into_contracts_economic_activities_and_transactions_complete_set_of_accounts ) ] ;
    rdfs:range xsd:boolean .

:resident a otv:Attribute,
        owl:DatatypeProperty ;
    rdfs:label "resident" ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( :Entity_entitlement_to_own_assets_ability_to_take_economic_decisions_ability_to_enter_into_contracts_economic_activities_and_transactions_complete_set_of_accounts ) ] ;
    rdfs:range xsd:boolean .

otv:attribute a owl:ObjectProperty ;
    rdfs:label "attribute" ;
    rdfs:comment "Link a concept to one of its attributes either inherited or specific" ;
    rdfs:domain otv:Concept ;
    rdfs:range otv:Attribute .

otv:beginDate a owl:AnnotationProperty ;
    rdfs:label "beginDate" ;
    rdfs:comment "Property that indicates the begin date of of an axis of analysis, concept, term, object" ;
    rdfs:domain otv:OTVCore ;
    rdfs:isDefinedBy "http://www.ontologia.fr/OTB/otv.rdf" ;
    rdfs:range xsd:dateTime .

otv:belongsToAxis a owl:ObjectProperty ;
    rdfs:label "belongsToAxis" ;
    rdfs:comment "Indicate the axis of analysis to which the difference belongs" ;
    rdfs:domain otv:Difference ;
    rdfs:range otv:AxisOfAnalysis .

otv:conceptName a owl:AnnotationProperty ;
    rdfs:label "conceptName" ;
    rdfs:comment "Name of the concept" ;
    rdfs:domain otv:Concept ;
    rdfs:isDefinedBy "http://www.ontologia.fr/OTB/otv.rdf" ;
    rdfs:range xsd:string .

otv:containsDifference a owl:ObjectProperty ;
    rdfs:label "containsDifference" ;
    rdfs:comment "Indicate the differences which belongs to the axis of analysis" ;
    rdfs:domain otv:AxisOfAnalysis ;
    rdfs:range otv:Difference .

otv:context a owl:AnnotationProperty ;
    rdfs:label "context" ;
    rdfs:comment "Context associated to the term" ;
    rdfs:domain otv:Term ;
    rdfs:isDefinedBy "http://www.ontologia.fr/OTB/otv.rdf" .

otv:denotedByProperName a owl:ObjectProperty ;
    rdfs:label "denotedByProperName" ;
    rdfs:comment "The denotedByPrperName property links an individual (object) to a proper name which denotes it" ;
    rdfs:domain otv:Object ;
    rdfs:isDefinedBy "http://www.ontologia.fr/OTB/otv.rdf" ;
    rdfs:range otv:ProperName .

otv:denotedByTerm a owl:ObjectProperty ;
    rdfs:label "denotedByTerm" ;
    rdfs:comment "Link a concept to a term which denotes it" ;
    rdfs:domain otv:Concept ;
    rdfs:isDefinedBy "http://www.ontologia.fr/OTB/otv.rdf" ;
    rdfs:range otv:Term .

otv:denotedConcept a owl:ObjectProperty ;
    rdfs:label "denotedConcept" ;
    rdfs:comment "Property whose value is the OTV concept denoted by the term: 'a term is a verbal designation of a concept' (ISO 1087)" ;
    rdfs:domain otv:Term ;
    rdfs:isDefinedBy "http://www.ontologia.fr/OTB/otv.rdf" ;
    rdfs:range otv:Concept .

otv:denotedObject a owl:ObjectProperty ;
    rdfs:label "denotedObject" ;
    rdfs:comment "Property whose value is the OTV object denoted by the proper name" ;
    rdfs:domain otv:ProperName ;
    rdfs:isDefinedBy "http://www.ontologia.fr/OTB/otv.rdf" ;
    rdfs:range otv:Object .

otv:difference a owl:ObjectProperty ;
    rdfs:label "difference" ;
    rdfs:comment "Link a concept to one of its differences either inherited or specific" ;
    rdfs:domain otv:Concept ;
    rdfs:range otv:Difference .

otv:endDate a owl:AnnotationProperty ;
    rdfs:label "endDate" ;
    rdfs:comment "Property that indicates the end date of of an axis of analysis, concept, term, object" ;
    rdfs:domain otv:OTVCore ;
    rdfs:isDefinedBy "http://www.ontologia.fr/OTB/otv.rdf" ;
    rdfs:range xsd:dateTime .

otv:gender a owl:AnnotationProperty ;
    rdfs:label "gender" ;
    rdfs:comment "Gender of the term" ;
    rdfs:domain otv:Term ;
    rdfs:isDefinedBy "http://www.ontologia.fr/OTB/otv.rdf" ;
    rdfs:range xsd:string .

otv:inheritedDifference a owl:ObjectProperty ;
    rdfs:label "inheritedDifference" ;
    rdfs:comment "Link a concept to one of its inherited differences" ;
    rdfs:domain otv:Concept ;
    rdfs:range otv:Difference .

otv:instanceOf a owl:ObjectProperty ;
    rdfs:label "instanceOf" ;
    rdfs:comment "instance relationship" ;
    rdfs:domain otv:Object ;
    rdfs:isDefinedBy "http://www.ontologia.fr/OTB/otv.rdf" ;
    rdfs:range otv:Concept .

otv:isA a owl:ObjectProperty ;
    rdfs:label "isA" ;
    rdfs:comment "Generic relationship between 2 concepts" ;
    rdfs:domain otv:Concept ;
    rdfs:isDefinedBy "http://www.ontologia.fr/OTB/otv.rdf" ;
    rdfs:range otv:Concept .

otv:language a owl:AnnotationProperty ;
    rdfs:label "language" ;
    rdfs:comment "Property that denotes the language of the term" ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( otv:Term otv:ProperName ) ] ;
    rdfs:isDefinedBy "http://www.ontologia.fr/OTB/otv.rdf" ;
    rdfs:range xsd:string .

otv:note a owl:AnnotationProperty ;
    rdfs:label "note" ;
    rdfs:comment "Note associated to the term" ;
    rdfs:domain otv:Term ;
    rdfs:isDefinedBy "http://www.ontologia.fr/OTB/otv.rdf" .

otv:ownDifference a owl:ObjectProperty ;
    rdfs:label "ownDifference" ;
    rdfs:comment "Link a concept to one of its specific differences" ;
    rdfs:domain otv:Concept ;
    rdfs:range otv:Difference .

otv:partOfSpeech a owl:AnnotationProperty ;
    rdfs:label "partOfSpeech" ;
    rdfs:comment "Part of Speech of the term" ;
    rdfs:domain otv:Term ;
    rdfs:isDefinedBy "http://www.ontologia.fr/OTB/otv.rdf" ;
    rdfs:range xsd:string .

otv:properName a owl:AnnotationProperty ;
    rdfs:label "properName" ;
    rdfs:comment "String representing the proper name" ;
    rdfs:domain otv:ProperName ;
    rdfs:isDefinedBy "http://www.ontologia.fr/OTB/otv.rdf" ;
    rdfs:range xsd:string .

otv:termDefinition a owl:AnnotationProperty ;
    rdfs:label "termDefinition" ;
    rdfs:comment "Defintion of the term in natural language" ;
    rdfs:domain otv:Term ;
    rdfs:isDefinedBy "http://www.ontologia.fr/OTB/otv.rdf" ;
    rdfs:range xsd:string .

otv:termName a owl:AnnotationProperty ;
    rdfs:label "termName" ;
    rdfs:comment "String representing the term" ;
    rdfs:domain otv:Term ;
    rdfs:isDefinedBy "http://www.ontologia.fr/OTB/otv.rdf" ;
    rdfs:range xsd:string .

otv:termStatus a owl:AnnotationProperty ;
    rdfs:label "termStatus" ;
    rdfs:comment "Property that indicates the status of the term" ;
    rdfs:domain otv:Term ;
    rdfs:isDefinedBy "http://www.ontologia.fr/OTB/otv.rdf" ;
    rdfs:range xsd:string .

