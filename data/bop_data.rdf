    <skos:Concept rdf:about="#Entity_entitlement_to_own_assets_ability_to_take_economic_decisions_ability_to_enter_into_contracts_economic_activities_and_transactions_complete_set_of_accounts_not_established_by_political_processes_no_authority_over_IUs_of_its_jurisdiction_existence_does_not_depend_on_persons_formed_as_a_legal_separate_entity_provision_of_financial_services_incuring_liabilities_on_its_own_account__for_the_purpose_of_acquiring_financial_assets_by_engaging_in_financial_transactions_on_the_market_in_order_to_collect_funds_from_lenders_and_transform_or_repackage_them_for_borrowers_providing_financial_services_other_than_insurance_where_most_of_either_their_assets_or_liabilities_are_not_transacted_on_open_financial_markets">
        <skos:inScheme rdf:resource="http://www.ontologia.fr/OTB/BOP"/>
        <rdfs:label xml:lang="de">Firmeneigene Finanzierungseinrichtung</rdfs:label>
        <skos:prefLabel xml:lang="de">Firmeneigene Finanzierungseinrichtung</skos:prefLabel>
        <skos:note xml:lang="de">SNA-Code: S.127.</skos:note>
        <rdfs:label xml:lang="en">captive financial institution</rdfs:label>
        <skos:prefLabel xml:lang="en">captive financial institution</skos:prefLabel>
        <skos:definition xml:lang="en">Financial intermediary providing financial services other than insurance, where most of either their assets or liabilities are not transacted on open financial markets.</skos:definition>
        <rdfs:label xml:lang="fr">institution financière captive</rdfs:label>
        <skos:prefLabel xml:lang="fr">institution financière captive</skos:prefLabel>
        <skos:definition xml:lang="fr">Intermédiaire financier offrant des services financiers hors assurance, et dont la plupart des actifs ou passifs ne sont pas échangés sur les marchés financiers ouverts.</skos:definition>
        <skos:note xml:lang="fr">Code SCN : S127.</skos:note>
        <skos:broader rdf:resource="#Entity_entitlement_to_own_assets_ability_to_take_economic_decisions_ability_to_enter_into_contracts_economic_activities_and_transactions_complete_set_of_accounts_not_established_by_political_processes_no_authority_over_IUs_of_its_jurisdiction_existence_does_not_depend_on_persons_formed_as_a_legal_separate_entity_provision_of_financial_services_incuring_liabilities_on_its_own_account__for_the_purpose_of_acquiring_financial_assets_by_engaging_in_financial_transactions_on_the_market_in_order_to_collect_funds_from_lenders_and_transform_or_repackage_them_for_borrowers"/>
    </skos:Concept>

    <skos:Concept rdf:about="#Principle_for_classification_concerning_transactions_positions_and_income_of_institutional_units_based_on_the_investment_relationship_between_parties_weak_degree_of_influence_through_debt_or_equity_securities">
        <skos:inScheme rdf:resource="http://www.ontologia.fr/OTB/BOP"/>
        <rdfs:label xml:lang="de">Portfolioinvestitionen</rdfs:label>
        <skos:altLabel xml:lang="de">Portfolioinvestitionen</skos:altLabel>
        <rdfs:label xml:lang="de">Wertpapieranlagen</rdfs:label>
        <skos:prefLabel xml:lang="de">Wertpapieranlagen</skos:prefLabel>
        <rdfs:label xml:lang="en">portfolio investment</rdfs:label>
        <skos:prefLabel xml:lang="en">portfolio investment</skos:prefLabel>
        <skos:definition xml:lang="en">Functional category corresponding to relationships in which a resident in an economy has a weak degree of influence on the management of an enterprise resident in another economy.</skos:definition>
        <rdfs:label xml:lang="fr">investissement de portefeuille</rdfs:label>
        <skos:prefLabel xml:lang="fr">investissement de portefeuille</skos:prefLabel>
        <skos:definition xml:lang="fr">Catégorie fonctionnelle correspondant aux relations matérialisées par des titres de créance ou des actions, dans lesquelles un résident d'une économie exerce une influence réduite sur la gestion d'une entreprise résidente d'une autre économie.</skos:definition>
        <skos:example xml:lang="fr">L’investissement de portefeuille suppose en général une infrastructure financière, par exemple un cadre légal, une réglementation et un dispositif de règlement appropriés, ainsi que des négociants teneurs de marché et un volume suffisant d’acheteurs et de vendeurs. (Source: FMI 2009, 6.56).</skos:example>
        <skos:broader rdf:resource="#Principle_for_classification_concerning_transactions_positions_and_income_of_institutional_units_based_on_the_investment_relationship_between_parties"/>
    </skos:Concept>

    <skos:Concept rdf:about="#Event_reflecting_the_creation_transformation_exchange_transfer_or_extinction_of_economic_value_involving_changes_in_the_volume_composition_or_value_of_an_institutional_units_assets_and_liabilities_giving_rise_to_entries_in_the_international_accounts_resulting_from_an_interaction_between_institutional_units_resident_in_different_economies_that_occurs_by_mutual_agreement_or_through_the_operation_of_the_law_distributing_the_value_added_generated_by_production_or_involving_the_redistribution_of_income_and_wealth_associated_with_the_redistribution_of_income_or_wealth">
        <skos:inScheme rdf:resource="http://www.ontologia.fr/OTB/BOP"/>
        <rdfs:label xml:lang="en">redistributive transaction</rdfs:label>
        <skos:prefLabel xml:lang="en">redistributive transaction</skos:prefLabel>
        <skos:definition xml:lang="en">Distributive transaction associated with the redistribution of income or wealth.</skos:definition>
        <rdfs:label xml:lang="fr">opération de redistribution</rdfs:label>
        <skos:prefLabel xml:lang="fr">opération de redistribution</skos:prefLabel>
        <skos:broader rdf:resource="#Event_reflecting_the_creation_transformation_exchange_transfer_or_extinction_of_economic_value_involving_changes_in_the_volume_composition_or_value_of_an_institutional_units_assets_and_liabilities_giving_rise_to_entries_in_the_international_accounts_resulting_from_an_interaction_between_institutional_units_resident_in_different_economies_that_occurs_by_mutual_agreement_or_through_the_operation_of_the_law_distributing_the_value_added_generated_by_production_or_involving_the_redistribution_of_income_and_wealth"/>
        <skos:narrower rdf:resource="#Event_reflecting_the_creation_transformation_exchange_transfer_or_extinction_of_economic_value_involving_changes_in_the_volume_composition_or_value_of_an_institutional_units_assets_and_liabilities_giving_rise_to_entries_in_the_international_accounts_resulting_from_an_interaction_between_institutional_units_resident_in_different_economies_that_occurs_by_mutual_agreement_or_through_the_operation_of_the_law_distributing_the_value_added_generated_by_production_or_involving_the_redistribution_of_income_and_wealth_associated_with_the_redistribution_of_income_or_wealth_associated_with_the_redistribution_of_income_"/>
